<?xml version="1.0" encoding="ISO-8859-1"?>
<web-app xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xmlns="http://java.sun.com/xml/ns/javaee" xmlns:web="http://java.sun.com/xml/ns/javaee/web-app_2_5.xsd" xsi:schemaLocation="http://java.sun.com/xml/ns/javaee http://java.sun.com/xml/ns/javaee/web-app_2_5.xsd" version="2.5">
  <filter>
    <display-name>Utf</display-name>
    <filter-name>Utf</filter-name>
    <filter-class>aa.bb.Utf</filter-class>
  </filter>
  <filter-mapping>
    <filter-name>Utf</filter-name>
    <url-pattern>/*</url-pattern>
  </filter-mapping>
  <filter>
    <display-name>ManagerType</display-name>
    <filter-name>ManagerType</filter-name>
    <filter-class>aa.bb.ManagerType</filter-class>
  </filter>
  <filter-mapping>
    <filter-name>ManagerType</filter-name>
    <url-pattern>/providerAdd.html</url-pattern>
    <url-pattern>/provider_0.jsp</url-pattern>
    <url-pattern>/userAdd.html</url-pattern>
    <url-pattern>/user_0.jsp</url-pattern>
    <url-pattern>/user_1.jsp</url-pattern>
  </filter-mapping>
  <filter>
    <display-name>ManagerTypeB</display-name>
    <filter-name>ManagerTypeB</filter-name>
    <filter-class>aa.bb.ManagerTypeB</filter-class>
  </filter>
  <filter-mapping>
    <filter-name>ManagerTypeB</filter-name>
    <url-pattern>/admin_index.html</url-pattern>
    <url-pattern>/providerAdd.html</url-pattern>
    <url-pattern>/provider_0.jsp</url-pattern>
    <url-pattern>/providerAdamin.jsp</url-pattern>
    <url-pattern>/userAdd.html</url-pattern>
    <url-pattern>/user_0.jsp</url-pattern>
    <url-pattern>/user_1.jsp</url-pattern>
    <url-pattern>/userAdmin.jsp</url-pattern>
    <url-pattern>/modify.jsp</url-pattern>
    <url-pattern>/modify_0.jsp</url-pattern>
    <url-pattern>/admin_bill_list.jsp</url-pattern>
  </filter-mapping>
  <servlet>
    <servlet-name>ModifyManager</servlet-name>
    <servlet-class>user.manager.ModifyManager</servlet-class>
  </servlet>
  <servlet-mapping>
    <servlet-name>ModifyManager</servlet-name>
    <url-pattern>/ModifyManager</url-pattern>
  </servlet-mapping>
  <servlet>
    <servlet-name>ProviderManager</servlet-name>
    <servlet-class>user.manager.ProviderManager</servlet-class>
  </servlet>
  <servlet-mapping>
    <servlet-name>ProviderManager</servlet-name>
    <url-pattern>/ProviderManager</url-pattern>
  </servlet-mapping>
  <servlet>
    <servlet-name>UserManager</servlet-name>
    <servlet-class>user.manager.UserManager</servlet-class>
  </servlet>
  <servlet-mapping>
    <servlet-name>UserManager</servlet-name>
    <url-pattern>/UserManager</url-pattern>
  </servlet-mapping>
  <servlet>
    <servlet-name>login</servlet-name>
    <servlet-class>login.user.UserLogin</servlet-class>
  </servlet>
  <servlet-mapping>
    <servlet-name>login</servlet-name>
    <url-pattern>/welcome</url-pattern>
  </servlet-mapping>
  <welcome-file-list>
    <welcome-file>login.jsp</welcome-file>
  </welcome-file-list>
</web-app>